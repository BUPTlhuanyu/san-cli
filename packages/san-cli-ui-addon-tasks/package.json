{
  "name": "san-cli-ui-addon-tasks",
  "version": "0.0.1",
  "private": true,
  "description": "san cli ui addon widgets",
  "scripts": {
    "start": "cross-env NODE_ENV=development ../san-cli/index.js serve src/index.js --config san.config.js",
    "build": "cross-env NODE_ENV=production ../san-cli/index.js build src/index.js --config san.config.js",
    "analyzer": "cross-env NODE_ENV=production ../san-cli/index.js build src/index.js --config san.config.js --analyze",
    "build:modern": "cross-env NODE_ENV=production ../san-cli/index.js build src/index.js --config san.config.js --modern",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "San",
    "San CLI"
  ],
  "license": "MIT",
  "devDependencies": {
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "babel-eslint": "^10.1.0",
    "prettier": "^2.0.5",
    "eslint-plugin-babel": "^5.3.0",
    "eslint": "^6.8.0",
    "@ecomfe/eslint-config": "^3.2.0",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "autoprefixer": "^9.7.5",
    "postcss-url": "^7.2.1",
    "san-cli": "^0.1.0"
  }
}
